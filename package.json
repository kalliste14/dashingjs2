{
  "name": "dashingjs2",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "commitlint": "commitlint -e",
    "cc": "npm run prettier && npm run lint",
    "pretty-quick": "pretty-quick",
    "prettier": "prettier --write src/**/*{ts,scss,md}",
    "test:cover": "rm -f ./coverage && ng test --no-watch --code-coverage",
    "coveralls": "cat ./coverage/dashingjs2/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "build:gh-pages:demo1": "ng build --configuration=demo1 --outputPath=gh-pages/demo1 --baseHref=/dashingjs2/demo1/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "bootstrap": "^4.4.1",
    "bourbon": "^6.0.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.21",
    "@angular/cli": "~8.3.21",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "angular-gridster2": "^8.2.0",
    "codelyzer": "^5.0.0",
    "coveralls": "^3.0.9",
    "font-awesome": "^4.7.0",
    "husky": "^3.1.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "node-sass": "^4.13.1",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pretty-quick --staged && npm run commitlint  && npm run lint"
    }
  },
  "browserslist": {
    "demo1": [
      "last 1 version",
      "> 1%"
    ],
    "production": [
      "last 2 Chrome version"
    ],
    "dev": [
      "last 2 Chrome version"
    ]
  }
}
